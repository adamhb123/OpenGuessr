</head>
<body>
<div id="viewer"></div>
<script>

    /* Photosphere */
    var viewer = new PhotoSphereViewer.Viewer({
        plugins: [[PhotoSphereViewer.MarkersPlugin, {markers: [
        {
        id: "br",
        longitude: '45deg',
        latitude: '45deg',
        rect:[450,45],
        },
      ]},]],
        container: document.querySelector('#viewer'),
        panorama: '/images/panoramas/download.png'
    });
    var markersPlugin = viewer.getPlugin(PhotoSphereViewer.MarkersPlugin);
    
    console.log("BRUH");
</script>
</body>
</html>
